<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>

  {% render 'layout.head.geo-redirect-script' %}
  {% render 'layout.head.redirects-script' %}
  {% render 'layout.head.meta-tags'%}
  {% render 'layout.head.canonicals-us' %}

  <!-- Stylesheets -->
  <link rel="preload" href="{{ 'common.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <noscript><link rel="stylesheet" href="{{ 'common.css' | asset_url }}"></noscript>
  <link rel="preload" href="{{ 'styles.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <noscript><link rel="stylesheet" href="{{ 'styles.css' | asset_url }}"></noscript>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"/>

  <!-- Swiper Import -->
  <link rel="preload" href="{{ 'swiper.min.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <script src="{{ 'swiper.min.js' | asset_url}}"></script>

  <!-- Script Libraries -->
  <script src="{{ 'jquery-3.6.0.min.js' | asset_url}}" ></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <script src="{{ 'libs.js' | asset_url }}" async></script>
  <script src="{{ 'common.js' | asset_url }}" async></script>
  <script type="module" src="https://cdn.jsdelivr.net/npm/@slightlyoff/lite-vimeo@0.1.1/lite-vimeo.js"></script>

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.16/css/intlTelInput.css" integrity="sha512-gxWow8Mo6q6pLa1XH/CcH8JyiSDEtiwJV78E+D+QP0EVasFs8wKXq16G8CLD4CJ2SnonHr4Lm/yY2fSI2+cbmw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.16/js/intlTelInput.min.js" integrity="sha512-Po9nSdYOcWIcoADdRjkAbRYPpR8OHjxzA/3RDUERZcDewTLzRTxbG4bUX7Sr7lVEcO3wTCzphdOBWgNFKVmxaA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

  <!-- Swiper Import -->
  <noscript><link rel="stylesheet" href="{{ 'swiper.min.css' | asset_url }}"></noscript>
  <script src="{{ 'swiper.min.js' | asset_url}}"></script>

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet"> 
  <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;1,300;1,400;1,500;1,600;1,700;1,800&display=swap" rel="stylesheet">
  <link rel="preload" href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <link href="https://fonts.googleapis.com/css2?family=Pacifico&display=swap" rel="stylesheet" />
  <link rel="stylesheet" href="{{ "icofont.min.css" | asset_url }}">
  <style>@font-face{font-family:IcoFont;font-weight:400;font-style:Regular;src:url({{ "icofont.woff2" | asset_url }}) format("woff2"),url({{ "icoFont.woff" | asset_url }}) format("woff")}</style>
  <style>@font-face{font-family:RadioGrotesk;font-weight:400;src:url({{ "PPRadioGrotesk-Regular.otf" | asset_url }})};@font-face{font-family:RadioGrotesk;font-weight:700;src:url({{ "PPRadioGrotesk-Bold.otf" | asset_url }});}@font-face{font-family:RadioGrotesk;font-weight:100;src:url({{ "PPRadioGrotesk-Light.otf" | asset_url }});}</style>
  <style>@font-face{font-family:DMSans;font-weight:400;src:url({{ "DMSans-Regular.ttf" | asset_url }});}@font-face{font-family:DMSans;font-weight:700;src:url({{ "DMSans-Bold.ttf" | asset_url }});}@font-face{font-family:DMSans;font-weight:500;src:url({{ "DMSans-Medium.ttf" | asset_url }});}</style>

  {% if template contains 'customers' %}
      {{ "shopify_common.js" | shopify_asset_url | script_tag }}
      {{ "customer_area.js"  | shopify_asset_url | script_tag }}
  {% endif %}
  {{ content_for_header }}  <!-- Header hook for plugins -->

  {{ 'option_selection.js' | shopify_asset_url | script_tag }}

  {% render 'layout.head.google-tag-manager'%}
  {% render 'layout.head.marker-io-snippet' %}
  {% render 'fa_translate_core' %}
  <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/5AyB8SFKOjnnor-g4qIeMg" async></script>
</head>
<body>
  <main role="main">
    {{ content_for_layout }}
  </main>

  <style>
    .shipscout-banner {
      display: none;
    }

    .locale-selectors__container {
      display: none;
    }
  </style>

  {% render 'layout.footer.pass-url-params-script' %}
  {% render 'layout.footer.woopra-snippet' %}
  {% render 'layout.footer.ac-snippet' %}
  {% render 'wisepops-generated' %}
</body>
</html>