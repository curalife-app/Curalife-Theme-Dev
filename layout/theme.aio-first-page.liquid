  <script>
    function runAfterElementExists(jquery_selector, callback) {
      var checker = window.setInterval(function () {
        if ($(jquery_selector).length) {
          clearInterval(checker);
          callback();
        }
      }, 200);
    }
  </script>

  <!-- This script is taking the URL parameter and replacing all the links on the page with the URL parameter. -->
  <script>
    let urlParameters = new URLSearchParams(window.location.search);
    let productVar = urlParameters.get('prod-var');

    if (productVar) {
    $('a').each(function() {
      var currLink = $(this).attr('href');
      if (currLink) { $(this).attr('href', "/products/curalin-pack-" + productVar) };
    });
    }
  </script>

  <!-- RAF -->
  <script>
    let urlParameter = new URLSearchParams(window.location.search);
    let linksAddition = urlParameter.get('utm_campaign');

    if (urlParameter.get('utm_campaign') == 'referral_program') {
      setTimeout(function () {
        $('a.btn-cta').each(function() {
          $(this).attr('href', $(this).attr('href').split('?')[0]);
        });
      }, 5000);

      // Wisepop Event!
      $(document).ready(function () {
        runAfterElementExists(".yotpo-copy-code-icon", function () {
          wisepops('properties', {rafCodeReceived: true});
          wisepops("pageview");

          $('a').each(function() {
            var currLink = $(this).attr('href');
            if (currLink) { $(this).attr('href', currLink + "-raf") };
          });
        })
      });
    }
  </script>

  <!-- Cta Buttons -->
  <script>
    async function LinkToProduct(element, url) {
      await woopra.track('Click CTA', {
        button_title: element.innerText
      });

      window.location.href = url;
    }
  </script>

  <!-- Woopra Events -->
  <script>
    $(document).ready(function () {
      // Play Video
      runAfterElementExists(".video-player", function () {
        $(".video-player").click(function() {
          woopra.track('Play Video', {
            video_id: $(this).find("lite-vimeo").attr("videoid"),
            video_title: $(this).siblings(".video-headline").find("p").text(),
            video_order: $(this).parents(".video-block-item-overlay-col").index() + 1
          });
        })
      })

      runAfterElementExists(".testimonials_iframe", function () {
        $(".testimonials_iframe").click(function() {
          woopra.track('Play Video', {
            video_id: $(this).find("lite-vimeo").attr("videoid"),
            video_title: $(this).siblings(".testimonial-headline").find("h2").text(),
          });
        })
      })

      // Open FAQ
      $('.question').click(function() {
        woopra.track('Open FAQ', {
          question_display_text: $(this).children("h4").text(),
          question_order:  $(this).parents("li").index() + 1
        })
      });
    })
  </script>

