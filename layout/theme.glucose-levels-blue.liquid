<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
    {%- endif -%}
  
 <link rel="preload" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"></noscript>
<link rel="preload" href="{{ 'bootstrap.min.vidc.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'bootstrap.min.vidc.css' | asset_url }}"></noscript>
<link rel="preload" href="{{ 'slick-vid.vidc.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'slick-vid.vidc.css' | asset_url }}"></noscript>
<link rel="preload" href="{{ 'style-vid-2020.vidc.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'style-vid-2020.vidc.css' | asset_url }}"></noscript>
<link rel="preload" href="{{ 'responsive.vidc.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'responsive.vidc.css' | asset_url }}"></noscript>
<link rel="preload" href="{{ 'common.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'common.css' | asset_url }}"></noscript>
<link rel="preload" href="{{ 'vids-glouse-bundle.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ 'vids-glouse-bundle.css' | asset_url }}"></noscript>
<script  src="{{ 'jquery.min.vidc.js' | asset_url }}"></script>
<script  src="{{ 'slick.min.vidc.js' | asset_url }}" async></script>
<script  src="{{ 'bootstrap.min.vidc.js' | asset_url }}" async></script>
<script  src="{{ 'custom-vidc.js' | asset_url }}" async></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/lazysizes/5.1.2/lazysizes.min.js" defer="defer"></script>

{% if template contains 'customers' %}
    {{ "shopify_common.js" | shopify_asset_url | script_tag }}
    {{ "customer_area.js"  | shopify_asset_url | script_tag }}
{% endif %}
{{ content_for_header }} <!-- Header hook for plugins -->

  <style>
   
    body{
      padding-top: 0;
    }
    div#shopify-section-vids-video-section .container, 
    div#shopify-section-vids-product-order-section-top .container,
    div#shopify-section-vids-benefits .container,
    div#shopify-section-vids-healthy-banner .container,
    div#shopify-section-vids-product-order-section-bottom .container,
    div#shopify-section-vids-company-logos .container
    {
    width: 100%;
    max-width: 1350px;
    margin: 0 auto;
}
.product-info{
  width: auto !important;
}
  </style>

  <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/5AyB8SFKOjnnor-g4qIeMg" async></script>
  <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/oMYBvmnASlHzmouicgkepg" async></script>
</head>
<body>

 {% render 'layout.head.google-tag-manager'%}
 {% render 'hot-jar'%}

  <main role="main">
    {{ content_for_layout }}
    
     {% section 'footer'%}
  </main>
 
</body>

</html>







