{% render 'product-faq-section',
  bgColor: {{ section.settings.bg_color }},
  title: {{ section.settings.title }},
  faqItems: faqItems
%}

{% schema %}
{
    "name": "FAQ",
    "disabled_on": {
      "groups": [
        "header",
        "footer"
      ]
    },
    "settings": [
      {
        "type": "color",
        "id": "bg_color",
        "label": "Background Color"
      },
      {
        "type": "text",
        "id": "title",
        "label": "Title"
      },
      {
        "type": "textarea",
        "id": "faq_data",
        "label": "FAQ Data"
      }
    ],
    "presets": [
    {
      "name": "FAQ"
    }
  ]
}
{% endschema %}