  <style>
    .video-reviews-section {
      padding: 50px 0;
    }

    .video-reviews-section h2 {
      font-size: 40px;
      margin: 0;
      margin-bottom: 30px;
    }

    .video-reviews-section .video-reviews {
      overflow: visible;
      max-width: 70%;
    }

    .video-reviews-section .swiper-button-prev, .video-reviews-section .swiper-button-next {
      color: gray;
      transition: opacity .5s;
    }

    .video-reviews-section .swiper-button-prev.disabled, .video-reviews-section .swiper-button-next.disabled {
      opacity: 0;
    }

    .video-reviews-section .swiper-button-prev::after, .video-reviews-section .swiper-button-next::after {
      font-size: 28px;
    }

    .video-reviews-section .video-reviews .video-review {
      display: flex;
      align-items: center;
    }

    .video-reviews-section .video-reviews .video-review .review-video {
      width: 40%;
      margin-left: 10%;
      height: 500px;
    }

    .video-reviews-section .video-reviews .video-review .review-video iframe {
      width: 100% !important;
      height: 100% !important;
    }

    .video-reviews-section .video-reviews .video-review .review-quote {
      padding: 50px;
      width: 50%;
      font-size: 22px;
      position: relative;
    }

    .video-reviews-section .video-reviews .video-review .review-quote img {
      opacity: .1;
      width: 70px;
      position: absolute;
      left: 30px;
      top: 20px;
    }

    .video-reviews-section .video-reviews .video-review .review-quote .quote-author {
      font-size: 18px;
      margin-bottom: 0;
    }

    @media screen and (max-width: 760px) {
      .video-reviews-section {
        background-color: #f0f0f0;
        padding: 8vw 0;
      }

      .video-reviews-section h2 {
        font-size: 6vw;
        text-align: center;
        max-width: unset;
        margin-bottom: 10px;
      }

      .video-reviews-section .video-reviews {
        max-width: unset;
        margin-top: 30px;
      }

      .video-reviews-section .video-reviews .video-review {
        flex-direction: column;
        max-width: 80%;
        margin: auto;
        text-align: center;
      }

      .video-reviews-section .video-reviews .video-review .review-video {
        width: unset;
        margin-left: unset;
        height: 300px;
        margin-bottom: 10px;
      }

      .video-reviews-section .video-reviews .video-review .review-quote {
        padding: unset;
        width: 100%;
        font-size: 16px;
      }

      .video-reviews-section .video-reviews .video-review .review-quote img {
        width: 40px;
        left: -10px;
        top: 0px;
      }

      .video-reviews-section .video-reviews .video-review .review-quote .quote-author {
        font-size: 14px;
      }
    }
  </style>

  <script>
    videoReviewsSliderAllInOne = new Swiper('.video-reviews-section .swiper', {
        slidesPerView: 'auto',
        spaceBetween: 8,
        centeredSlides: true,
        grabCursor: true,
        slideToClickedSlide: true,
        loop: false,
        navigation: {
            nextEl: '.video-reviews-section .swiper-button-next',
            prevEl: '.video-reviews-section .swiper-button-prev',
            disabledClass: 'disabled'
        }
    });
  </script>

<section class="ingredients-section" id="powerful-ingredients">
    <div class="container no-padding">
    <h2><b>9 poderosos ingredientes</b><br>combinados en una cápsula</h2>
    <div class="ingredients-sliders">
        <div class="ingredients-thumbs-slider swiper">
          <div class="ingredients swiper-wrapper">
            {% for i in (1..9) %}
