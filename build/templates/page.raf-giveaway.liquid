              prevEl: '.swiper-button-prev',
              disabledClass: 'disabled'
          },
          autoplay: {
              delay: 3000,
          },
          breakpoints: {
              0: {
                  slidesPerView: 1,
              },
              630: {
                  slidesPerView: 1,
              },
              900: {
                  slidesPerView: 3,
              }
          }
      });
  </script>

  <div class="floating-cta">
    <button class="btn-cta">ENTER NOW</button>
  </div>

  <style>
    .floating-cta {
      position: fixed;
      left: 20px;
      bottom: 20px;
      z-index: 20;
      filter: drop-shadow(0px 4px 4px rgba(0, 0, 0, 0.25));
      display: none;
    }

    .floating-cta .btn-cta {
      font-size: 28px;
      pointer-events: auto;
      color: black;
    }

    @media screen and (max-width: 768px) {
      .floating-cta .btn-cta {
        font-size: 5vw;
        padding: 4vw 6vw;
      }
    }
  </style>

  <script>
    jQuery(document).ready(function() {
        var offset = 300;
        var duration = 500;
        jQuery(window).scroll(function() {
            if (jQuery(this).scrollTop() > offset) {
                jQuery('.floating-cta').fadeIn(duration);
            } else {
                jQuery('.floating-cta').fadeOut(duration);
            }
        });

        jQuery('.floating-cta').click(function(event) {
            event.preventDefault();
            jQuery('html, body').animate({scrollTop: 0}, duration);
            return false;
        })
    });
  </script>
</div>

