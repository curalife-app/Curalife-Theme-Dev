  <script>
    function runAfterElementExists(jquery_selector,callback){
        var checker = window.setInterval(function() {
        if ($(jquery_selector).length) {
            clearInterval(checker);
            callback();
        }}, 200);
    }

    $( document ).ready(function() {
      runAfterElementExists(".yotpo-referral-widget-form", function() {
        $(".yotpo-action-button-widget button").click(function() {
          // Track in GA
          window.dataLayer = window.dataLayer || [];
          window.dataLayer.push({
          'event': 'referrer-submit'
          });
        })
      });

      runAfterElementExists(".yotpo-screen-friends_emails", function() {
        $(".yotpo-friends-share-button button").click(function() {
