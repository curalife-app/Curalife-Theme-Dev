	"scripts": {
		"prettier": "prettier",
		"ðŸš€ --- UNIFIED BUILD SYSTEM ---": "",
		"dev": "node build-scripts/curalife.js dev",
		"build": "npm run transpile:quiz && node build-scripts/curalife.js build",
		"watch": "node build-scripts/curalife.js watch",
		"shopify": "node build-scripts/curalife.js shopify",
		"shopify:dev": "node build-scripts/curalife.js shopify --build-first",
		"shopify-urls": "node build-scripts/curalife.js shopify-urls",
		"optimize": "node build-scripts/curalife.js optimize",
		"analyze": "node build-scripts/curalife.js analyze",
		"config": "node build-scripts/curalife.js config",
		"clean": "node build-scripts/curalife.js clean",
		"ðŸ—ï¸ --- LEGACY BUILD SYSTEM ---": "",
		"build:legacy": "node build-scripts/build-complete.js",
		"build:report": "node build-scripts/build-complete.js --report",
		"build:optimize": "npm run build:legacy && node build-scripts/build-utilities/optimize-assets.js",
		"transpile:quiz": "node build-scripts/transpile-quiz-simple.cjs",
		"watch:legacy": "node build-scripts/watch.js",
		"watch:report": "node build-scripts/watch.js --report",
		"watch:shopify": "node build-scripts/watch-shopify.js",
		"watch:shopify:debug": "node build-scripts/watch-shopify.js --debug",
		"ðŸŽ® --- TUI INTERFACES ---": "",
		"tui": "go run cmd/curalife-tui/main.go cmd/curalife-tui/backend.go",
		"tui:build": "go build -o bin/curalife-tui cmd/curalife-tui/main.go cmd/curalife-tui/backend.go",
		"build:tui": "node build-scripts/tui-adapters/build-adapter.js",
		"watch:tui": "node build-scripts/tui-adapters/watch-adapter.js",
		"watch:shopify:tui": "node build-scripts/tui-adapters/watch-adapter.js --shopify",
		"shopify:login": "shopify auth login",
		"shopify:theme:list": "cd Curalife-Theme-Build && shopify theme list",
		"shopify:theme:push": "shopify theme push --path Curalife-Theme-Build",
		"shopify:theme:pull": "shopify theme pull --path Curalife-Theme-Build",
		"shopify:theme:dev": "shopify theme dev --path Curalife-Theme-Build",
		"shopify:theme:open": "cd Curalife-Theme-Build && shopify theme open",
		"check-server": "node testing/lighthouse/check-server.js",
		"lighthouse:ci": "node testing/lighthouse/check-server.js && npx lhci autorun --config=./testing/lighthouse/lighthouserc.cjs",
		"lighthouse:desktop": "node testing/lighthouse/check-server.js && npx lighthouse http://localhost:9292 --view --preset=desktop",
		"lighthouse:mobile": "node testing/lighthouse/check-server.js && npx lighthouse http://localhost:9292 --view --preset=mobile",
		"performance:baseline": "node testing/lighthouse/check-server.js && npx lighthouse http://localhost:9292 --output=html --preset=desktop --output-path=./testing/lighthouse/performance-reports/baseline-desktop.html && npx lighthouse http://localhost:9292 --output=html --preset=mobile --output-path=./testing/lighthouse/performance-reports/baseline-mobile.html",
		"performance:compare": "node testing/lighthouse/check-server.js && npx lighthouse http://localhost:9292 --output=html --preset=desktop --output-path=./testing/lighthouse/performance-reports/current-desktop.html && npx lighthouse http://localhost:9292 --output=html --preset=mobile --output-path=./testing/lighthouse/performance-reports/current-mobile.html",
		"performance:monitor": "node testing/lighthouse/performance-monitor.js",
		"lint": "node linting/lint.js",
		"lint:fix": "node linting/lint.js --fix",
		"lint:shopify": "node linting/check-shopify-commands.js",
		"analyze:liquid": "node linting/analyze-liquid-js.js",
		"eslint:add-comments": "node linting/add-eslint-comments.js",
		"cleanup": "powershell -ExecutionPolicy Bypass -File scripts/cleanup-processes.ps1",
		"cleanup:force": "taskkill /F /IM node.exe 2>nul || echo No Node processes found",
		"status": "powershell -Command \"Get-Process -Name node -ErrorAction SilentlyContinue | Where-Object { (Get-WmiObject Win32_Process -Filter \\\"ProcessId = $($_.Id)\\\").CommandLine -match 'watch|tui|curalife' } | Format-Table Id, ProcessName, StartTime\""
	},
	"packageManager": "pnpm@10.4.1",
	"shopify": {
		"notes": "Before using the Shopify integration, make sure to:",
		"step1": "Install either the new Shopify CLI (@shopify/cli @shopify/theme) or ThemeKit (@shopify/themekit)",
		"step2": "Authenticate using 'npm run shopify:login' or configure config.yml for ThemeKit",
		"step3": "For ThemeKit, set up your API credentials in the config.yml file",
		"docs": "See SHOPIFY-DEV.md for detailed setup instructions"
	}
