<style>
{{ settings.type_body_font | font_face: font_display: 'swap' }}
@font-face {
  font-family: 'PP Radio Grotesk';
  font-style: light;
  font-weight: 300;
  src: url('{{ "PPRadioGrotesk-Light.otf" | asset_url }}');
  src: local(''),
  url('{{ "PPRadioGrotesk-Light.otf" | asset_url }}') format('otf'),
}

@font-face {
  font-family: 'PP Radio Grotesk';
  font-style: normal;
  font-weight: 400;
  src: url('{{ "PPRadioGrotesk-Regular.otf" | asset_url }}');
  src: local(''),
  url('{{ "PPRadioGrotesk-Regular.otf" | asset_url }}') format('otf'),
}

@font-face {
  font-family: 'PP Radio Grotesk';
  font-style: bold;
  font-weight: 700;
  src: url('{{ "PPRadioGrotesk-Bold.otf" | asset_url }}');
  src: local(''),
  url('{{ "PPRadioGrotesk-Bold.otf" | asset_url }}') format('otf'),
}

@font-face {
  font-family: 'DM Sans';
  font-style: normal;
  font-weight: 400;
  src: url('{{ "DMSans-Regular.ttf" | asset_url }}');
  src: local(''),
  url('{{ "DMSans-Regular.ttf" | asset_url }}') format('ttf'),
}

@font-face {
  font-family: 'DM Sans';
  font-style: normal;
  font-weight: 500;
  src: url('{{ "DMSans-Medium.ttf" | asset_url }}');
  src: local(''),
  url('{{ "DMSans-Medium.ttf" | asset_url }}') format('ttf'),
}

@font-face {
  font-family: 'DM Sans';
  font-style: bold;
  font-weight: 700;
  src: url('{{ "DMSans-Bold.ttf" | asset_url }}');
  src: local(''),
  url('{{ "DMSans-Bold.ttf" | asset_url }}') format('ttf'),
}



@font-face {
  font-family: 'icoFont';
  src: url("{{ 'icoFont.eot' | asset_url }}");
  src: url("{{ 'icoFont.eot' | asset_url }}?uayl2p#iefix") format("embedded-opentype"),
  url("{{ 'icoFont.ttf' | asset_url }}?uayl2p") format("truetype"),
  url("{{ 'icoFont.woff' | asset_url }}?uayl2p") format("woff"),
  url("{{ 'icoFont.svg' | asset_url }}?uayl2p#icoFont") format("svg");
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

@font-face {
  font-family: 'Ticking Timebomb';
  font-style: normal;
  font-weight: 400;
  src: url('{{ "TickingTimebombBB.ttf" | asset_url }}');
  src: local(''),
  url('{{ "TickingTimebombBB.ttf" | asset_url }}') format('ttf'),
}
