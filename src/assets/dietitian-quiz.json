						{ "id": "WY", "text": "Wyoming" }
					]
				},
				{
					"id": "q6_month",
					"text": "Date of birth",
					"type": "date-part",
					"part": "month",
					"required": true,
					"placeholder": "Month"
				},
				{
					"id": "q6_day",
					"text": "",
					"type": "date-part",
					"part": "day",
					"required": true,
					"placeholder": "Day"
				},
				{
					"id": "q6_year",
					"text": "",
					"type": "date-part",
					"part": "year",
					"required": true,
					"placeholder": "Year"
				},
				{
					"id": "q7",
					"text": "First name",
					"type": "text",
					"required": true,
					"placeholder": "Type your first name",
					"validation": {
						"pattern": "^[A-Za-z\\s]{1,100}$",
						"message": "Use only A–Z letters and spaces"
					}
				},
				{
					"id": "q8",
					"text": "Last name",
					"type": "text",
					"required": true,
					"placeholder": "Type your last name",
					"validation": {
						"pattern": "^[A-Za-z\\s]{1,100}$",
						"message": "Use only A–Z letters and spaces"
					}
				}
