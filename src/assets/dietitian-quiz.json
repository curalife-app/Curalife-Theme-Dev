						{ "id": "st11", "text": "Georgia" },
						{ "id": "st12", "text": "Hawaii" },
						{ "id": "st13", "text": "Idaho" },
						{ "id": "st14", "text": "Illinois" },
						{ "id": "st15", "text": "Indiana" },
