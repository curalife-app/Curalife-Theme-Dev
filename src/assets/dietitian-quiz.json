				},
				{
					"id": "q6",
					"text": "Birthday",
					"type": "date",
					"required": true
				},
				{ "id": "q7", "text": "First name", "type": "text", "required": true },
				{ "id": "q8", "text": "Last name", "type": "text", "required": true }
			],
			"ctaText": "Continue",
			"legal": "By clicking continue, you acknowledge that you have read, understood, and accepted the Curalife Terms of Service and Privacy Policy."
		},
		{
			"id": "step-contact",
			"info": {
				"heading": "How can we reach you?",
				"text": "We'll send your benefit results and help you schedule with a dietitian."
			},
			"questions": [
				{
					"id": "q9",
					"text": "Email",
					"type": "text",
					"required": true,
					"placeholder": "Enter your email",
					"validation": {
						"pattern": "^[\\w.+\\-]+@([\\w\\-]+\\.)+[A-Za-z]{2,}$",
						"message": "Please enter a valid email address."
					}
				},
				{
					"id": "q10",
					"text": "Phone number",
					"type": "text",
					"required": true,
					"placeholder": "Enter your phone",
					"validation": {
						"pattern": "^\\+?[0-9\\s\\-()]{7,}$",
						"message": "Please enter a valid phone number."
					}
				}
			],
			"ctaText": "Continue",
			"legal": "By clicking continue, you agree to receive benefit results and occasional marketing texts from Curalife. You can opt-out anytime."
		},
		{
			"id": "step-eligibility",
			"info": {
				"heading": "Your Insurance Benefits",
				"text": "We've checked your insurance benefits based on the information you provided.",
				"subtext": "93% of participants see success with our programs."
			},
			"eligibilityData": {
				"eligible": "{{eligibilityData.isEligible}}",
				"sessionsCovered": "{{eligibilityData.sessionsCovered}}",
				"deductible": "{{eligibilityData.deductible.individual}}",
				"coverageActive": "{{eligibilityData.coverageActive}}",
				"message": "{{eligibilityData.userMessage}}",
				"status": "{{eligibilityData.eligibilityStatus}}"
			},
			"ctaText": "Proceed to appointment booking →"
		}
	],

	"recommendations": []
}
