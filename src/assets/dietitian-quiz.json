						{ "id": "st17", "text": "Kansas" },
						{ "id": "st18", "text": "Kentucky" },
						{ "id": "st19", "text": "Louisiana" },
						{ "id": "st20", "text": "Maine" },
						{ "id": "st21", "text": "Maryland" },
						{ "id": "st22", "text": "Massachusetts" },
						{ "id": "st23", "text": "Michigan" },
