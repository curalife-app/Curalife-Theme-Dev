{
	"id": "curalife-intake",
	"title": "Find Your Perfect Dietitian",
	"description": "Answer a few quick questions so we can match you with the right registered dietitian, confirm your insurance benefits, and help you pay $0 out-of-pocket.",
	"steps": [
		{
			"id": "step-intro",
			"info": {
				"heading": "Your first step toward balanced blood sugar – the natural way.",
				"text": "Over the next few minutes we'll match you with a licensed dietitian and make sure you're eligible to pay $0 out-of-pocket."
			},
			"ctaText": "Continue"
		},
		{
			"id": "step-goals",
			"questions": [
				{
					"id": "q1",
					"text": "What's the main reason for your visit?",
					"type": "multiple-choice",
					"required": true,
					"helpText": "We'll use this to find your perfect match.",
					"options": [
						{ "id": "opt1", "text": "Weight Loss" },
						{ "id": "opt2", "text": "Blood Sugar Health" },
						{ "id": "opt3", "text": "Supplements Advice" },
						{ "id": "opt4", "text": "Other" }
					]
				},
				{
					"id": "q2",
					"text": "What else can we support you with?",
					"type": "checkbox",
					"required": false,
					"helpText": "Choose all that apply.",
					"options": [
						{ "id": "opt5", "text": "Weight Loss" },
						{ "id": "opt6", "text": "Blood Sugar Health" },
						{ "id": "opt7", "text": "Supplements Advice" },
						{ "id": "opt8", "text": "Glucometer & Strips" }
					]
				}
			],
			"ctaText": "Continue"
		},
		{
			"id": "step-insurance",
			"info": {
				"heading": "Let's check your insurance benefits",
				"text": "94% of patients have coverage and pay $0 out of pocket. Enter the details as they appear on your insurance card."
			},
			"questions": [
				{
					"id": "q3",
					"text": "Insurance plan",
					"type": "dropdown",
					"required": true,
					"options": [
						{ "id": "ins1", "text": "Aetna" },
						{ "id": "ins2", "text": "Anthem" },
						{ "id": "ins3", "text": "Blue Cross Blue Shield" },
						{ "id": "ins4", "text": "Cigna" },
						{ "id": "ins5", "text": "Humana" },
						{ "id": "ins6", "text": "Kaiser Permanente" },
						{ "id": "ins7", "text": "Molina Healthcare" },
						{ "id": "ins8", "text": "UnitedHealthcare" },
						{ "id": "ins9", "text": "Health Net" },
						{ "id": "ins10", "text": "Highmark" },
						{ "id": "ins11", "text": "Tricare" },
						{ "id": "ins12", "text": "Medicare" },
						{ "id": "ins13", "text": "Medicaid" },
						{ "id": "ins14", "text": "Other / Not Listed" }
					]
				},
				{
					"id": "q4",
					"text": "Member ID",
					"type": "text",
					"required": true,
					"placeholder": "Enter your member ID",
					"validation": {
						"pattern": "^[A-Za-z0-9]{3,}$",
						"message": "Please enter a valid member ID."
					}
				},
				{
					"id": "q5",
					"text": "State",
					"type": "dropdown",
					"required": true,
					"options": [
						{ "id": "st1", "text": "Alabama" },
						{ "id": "st2", "text": "Alaska" },
						{ "id": "st3", "text": "Arizona" },
						{ "id": "st4", "text": "Arkansas" },
