{
	"id": "curalife-intake",
	"title": "Find Your Perfect Dietitian",
	"description": "Answer a few quick questions so we can match you with the right registered dietitian, confirm your insurance benefits, and help you pay $0 out-of-pocket.",
	"config": {
		"version": "1.0",
		"totalSteps": 5,
		"estimatedTime": "3-5 minutes"
	},
	"steps": [
		{
			"id": "step-goals",
			"title": "Visit Goals",
			"info": {
				"heading": "What's the main reason for your visit?",
				"text": "We'll use this to find your perfect match."
			},
			"questions": [
				{
					"id": "q1",
					"text": "Select one",
					"type": "multiple-choice",
					"required": true,
					"options": [
						{ "id": "opt1", "text": "Weight Loss" },
						{ "id": "opt2", "text": "Blood Sugar Health" },
						{ "id": "opt3", "text": "Supplements Advice" },
						{ "id": "opt4", "text": "Blood Pressure Health" }
					]
				}
			],
			"ctaText": "Continue"
		},
		{
			"id": "step-medical",
			"title": "Medical History",
			"questions": [
				{
					"id": "q2",
					"text": "What else can we support you with?",
					"info": "Select all that apply so we can match you with the right dietitian.",
					"type": "checkbox",
					"required": true,
					"options": [
						{ "id": "med1", "text": "Type 1 Diabetes" },
						{ "id": "med2", "text": "Type 2 Diabetes" },
						{ "id": "med3", "text": "Prediabetes" },
						{ "id": "med4", "text": "Insulin resistance" },
						{ "id": "med5", "text": "High blood pressure" },
						{ "id": "med6", "text": "PCOS" },
						{ "id": "med7", "text": "None of the above" }
					]
				}
			],
			"ctaText": "Continue"
		},
		{
			"id": "step-insurance",
			"title": "Insurance Information",
			"info": {
				"heading": "Let's check your insurance benefits",
				"text": "We need some info to check your coverage and show you dietitians who are in-network.",
				"formSubHeading": "Please enter information as it appears on insurance card"
			},
			"questions": [
				{
					"id": "q3",
					"text": "Insurance plan",
					"type": "dropdown",
					"required": true,
					"placeholder": "Select plan",
					"options": [
						{ "id": "ins1", "text": "Aetna" },
						{ "id": "ins15", "text": "Ambetter" },
						{ "id": "ins2", "text": "Anthem" },
						{ "id": "ins3", "text": "Blue Cross Blue Shield" },
						{ "id": "ins4", "text": "Cigna" },
						{ "id": "ins5", "text": "Humana" },
						{ "id": "ins6", "text": "Kaiser Permanente" },
						{ "id": "ins7", "text": "Molina Healthcare" },
						{ "id": "ins8", "text": "UnitedHealthcare" },
						{ "id": "ins9", "text": "Health Net" },
						{ "id": "ins10", "text": "Highmark" },
						{ "id": "ins11", "text": "Tricare" },
						{ "id": "ins12", "text": "Medicare" },
						{ "id": "ins13", "text": "Medicaid" },
						{ "id": "ins14", "text": "Other / Not Listed" }
					]
				},
				{
					"id": "q4",
					"text": "Member ID",
					"type": "text",
					"required": true,
					"placeholder": "XYZ123456789",
					"validation": {
						"pattern": "^.{6,20}$",
						"message": "Please enter a valid member ID (6-20 characters)."
					}
				},
				{
					"id": "q4_group",
					"text": "Group number (if applicable)",
					"type": "text",
					"required": false,
					"placeholder": "Group number",
					"validation": {
						"pattern": "^[A-Za-z0-9\\s\\-]{0,20}$",
						"message": "Please enter a valid group number (letters, numbers, spaces, and hyphens only)."
					}
				},
				{
					"id": "q5",
					"text": "State",
					"type": "dropdown",
					"required": true,
					"placeholder": "Select state",
					"options": [
						{ "id": "AL", "text": "AL - Alabama" },
						{ "id": "AK", "text": "AK - Alaska" },
						{ "id": "AZ", "text": "AZ - Arizona" },
						{ "id": "AR", "text": "AR - Arkansas" },
						{ "id": "CA", "text": "CA - California" },
						{ "id": "CO", "text": "CO - Colorado" },
						{ "id": "CT", "text": "CT - Connecticut" },
						{ "id": "DE", "text": "DE - Delaware" },
						{ "id": "DC", "text": "DC - District of Columbia" },
						{ "id": "FL", "text": "FL - Florida" },
						{ "id": "GA", "text": "GA - Georgia" },
						{ "id": "HI", "text": "HI - Hawaii" },
						{ "id": "ID", "text": "ID - Idaho" },
						{ "id": "IL", "text": "IL - Illinois" },
						{ "id": "IN", "text": "IN - Indiana" },
						{ "id": "IA", "text": "IA - Iowa" },
						{ "id": "KS", "text": "KS - Kansas" },
						{ "id": "KY", "text": "KY - Kentucky" },
						{ "id": "LA", "text": "LA - Louisiana" },
						{ "id": "ME", "text": "ME - Maine" },
						{ "id": "MD", "text": "MD - Maryland" },
						{ "id": "MA", "text": "MA - Massachusetts" },
						{ "id": "MI", "text": "MI - Michigan" },
						{ "id": "MN", "text": "MN - Minnesota" },
						{ "id": "MS", "text": "MS - Mississippi" },
						{ "id": "MO", "text": "MO - Missouri" },
						{ "id": "MT", "text": "MT - Montana" },
						{ "id": "NE", "text": "NE - Nebraska" },
						{ "id": "NV", "text": "NV - Nevada" },
						{ "id": "NH", "text": "NH - New Hampshire" },
						{ "id": "NJ", "text": "NJ - New Jersey" },
						{ "id": "NM", "text": "NM - New Mexico" },
						{ "id": "NY", "text": "NY - New York" },
						{ "id": "NC", "text": "NC - North Carolina" },
						{ "id": "ND", "text": "ND - North Dakota" },
						{ "id": "OH", "text": "OH - Ohio" },
						{ "id": "OK", "text": "OK - Oklahoma" },
						{ "id": "OR", "text": "OR - Oregon" },
						{ "id": "PA", "text": "PA - Pennsylvania" },
						{ "id": "RI", "text": "RI - Rhode Island" },
						{ "id": "SC", "text": "SC - South Carolina" },
						{ "id": "SD", "text": "SD - South Dakota" },
						{ "id": "TN", "text": "TN - Tennessee" },
						{ "id": "TX", "text": "TX - Texas" },
						{ "id": "UT", "text": "UT - Utah" },
						{ "id": "VT", "text": "VT - Vermont" },
						{ "id": "VA", "text": "VA - Virginia" },
						{ "id": "WA", "text": "WA - Washington" },
						{ "id": "WV", "text": "WV - West Virginia" },
						{ "id": "WI", "text": "WI - Wisconsin" },
						{ "id": "WY", "text": "WY - Wyoming" }
					]
				},
				{
					"id": "q6_month",
					"text": "Date of birth",
					"type": "date-part",
					"part": "month",
					"required": true,
					"placeholder": "Month"
				},
				{
					"id": "q6_day",
					"text": "",
					"type": "date-part",
					"part": "day",
					"required": true,
					"placeholder": "Day"
				},
				{
					"id": "q6_year",
					"text": "",
					"type": "date-part",
					"part": "year",
					"required": true,
					"placeholder": "Year"
				},
				{
					"id": "q7",
					"text": "First name",
					"type": "text",
					"required": true,
					"placeholder": "Type your first name",
					"validation": {
						"pattern": "^[A-Za-z\\s\\-']{1,100}$",
						"message": "Please enter a valid first name (letters, spaces, hyphens, and apostrophes only)."
					}
				},
				{
					"id": "q8",
					"text": "Last name",
					"type": "text",
					"required": true,
					"placeholder": "Type your last name",
					"validation": {
						"pattern": "^[A-Za-z\\s\\-']{1,100}$",
						"message": "Please enter a valid last name (letters, spaces, hyphens, and apostrophes only)."
					}
				}
			],
			"ctaText": "Continue",
			"legal": "By clicking continue, you acknowledge that you have read, understood, and accepted the Curalife <a href=\"https://curalife.com/policies/terms-of-service\" target=\"_blank\" rel=\"noopener noreferrer\">Terms of Service</a> and <a href=\"https://curalife.com/policies/privacy-policy\" target=\"_blank\" rel=\"noopener noreferrer\">Privacy Policy</a>."
		},
		{
			"id": "step-contact",
			"title": "Contact Information",
			"info": {
				"heading": "Let's introduce each other",
				"text": "We'll use this to personalize your experience and follow up with your custom results."
			},
			"questions": [
				{
					"id": "q9",
					"text": "Email",
					"type": "text",
					"required": true,
					"placeholder": "Enter your email",
					"validation": {
						"pattern": "^[\\w.+\\-]+@([\\w\\-]+\\.)+[A-Za-z]{2,}$",
						"message": "Please enter a valid email address."
					}
				},
				{
					"id": "q10",
					"text": "Phone number",
					"type": "text",
					"required": true,
					"placeholder": "Enter your phone",
					"validation": {
						"pattern": "^\\+?[0-9\\s\\-()]{7,}$",
						"message": "Please enter a valid phone number."
					}
				}
			],
			"ctaText": "Get your estimated price",
			"legal": "By clicking continue, you agree to receive benefit results and occasional marketing texts from Curalife. You can opt-out anytime."
		},
		{
			"id": "step-eligibility",
			"title": "Insurance Benefits",
			"info": {
				"heading": "Your Insurance Benefits",
				"text": "We've checked your insurance benefits based on the information you provided.",
				"subtext": "93% of participants see success with our programs."
			},
			"eligibilityData": {
				"eligible": "{{eligibilityData.isEligible}}",
				"sessionsCovered": "{{eligibilityData.sessionsCovered}}",
				"deductible": "{{eligibilityData.deductible.individual}}",
				"copay": "{{eligibilityData.copay}}",
				"planBegin": "{{eligibilityData.planBegin}}",
				"planEnd": "{{eligibilityData.planEnd}}",
				"coverageActive": "{{eligibilityData.coverageActive}}",
				"message": "{{eligibilityData.userMessage}}",
				"status": "{{eligibilityData.eligibilityStatus}}"
			},
			"ctaText": "Proceed to appointment booking â†’"
		}
	],
