						{ "id": "st33", "text": "New York" },
						{ "id": "st34", "text": "North Carolina" },
						{ "id": "st35", "text": "North Dakota" },
						{ "id": "st36", "text": "Ohio" },
						{ "id": "st37", "text": "Oklahoma" },
						{ "id": "st38", "text": "Oregon" },
						{ "id": "st39", "text": "Pennsylvania" },
						{ "id": "st40", "text": "Rhode Island" },
						{ "id": "st41", "text": "South Carolina" },
						{ "id": "st42", "text": "South Dakota" },
						{ "id": "st43", "text": "Tennessee" },
						{ "id": "st44", "text": "Texas" },
						{ "id": "st45", "text": "Utah" },
						{ "id": "st46", "text": "Vermont" },
