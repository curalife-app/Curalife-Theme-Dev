<script>
  window.addEventListener('scroll', (e) => {
      let last_known_scroll_position = window.scrollY;
      if (counters && animateCounters && window.innerHeight > document.querySelector('.cta').getBoundingClientRect().top && window.innerHeight < document.querySelector('.cta').getBoundingClientRect().top + document.querySelector('.cta button').getBoundingClientRect().top) {
          animateCounters = false;
          counters.forEach(counter => {
              const animate = () => {
                  const value = parseFloat(+counter.getAttribute('data-count'));
                  const dataLine = counter.closest('li').querySelector('.line span');
                  const data = +counter.innerText;

                  const time = value / countingSpeed;
                  if (data < value) {
                      counter.innerText = Math.ceil(data + time);
                      dataLine.style.width = Math.ceil(data + time) + '%';
                      setTimeout(animate, 0.1);
                  } else {
                      counter.innerText = value;
                      dataLine.style.width = value + '%';
                  }
              }
              animate();
          });
      }

      if ((document.querySelector('.meet-curalin').getBoundingClientRect().top < -(document.querySelector('.meet-curalin .meet-curalin-content').getBoundingClientRect().height / 2) || document.querySelector('.meet-curalin').getBoundingClientRect().top < -(document.querySelector('.meet-curalin .meet-curalin-content').getBoundingClientRect().height / 2)) &&
          window.innerHeight < document.querySelector('.cta button').getBoundingClientRect().top) {
          document.querySelector('.floating-cta').classList.add('show');
      } else if (document.querySelector('.cta button').getBoundingClientRect().top < -document.querySelector('.cta button').getBoundingClientRect().height && !document.querySelector('.floating-cta').classList.contains("active")) {
          document.querySelector('.floating-cta').classList.add('show');
      } else {
          document.querySelector('.floating-cta').classList.remove('show');
      }
  });
</script>