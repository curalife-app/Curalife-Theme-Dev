<style>
  .btn-cta {
    border: none;
  }
  .floating-cta {
    position: fixed;
    left: 20px;
    right: 0;
    bottom: 1.25rem;
    z-index: 20;
    pointer-events: none;
    transition: opacity var(--transition), visibility var(--transition);
  }

  .floating-cta .btn-cta {
    max-width: 18.375rem;
    font-size: 2.5rem;
    pointer-events: auto;
  }

  .floating-cta:not(.show) {
    opacity: 0;
    visibility: hidden;
  }

  .floating-cta:not(.show) .btn-cta {
    pointer-events: none;
  }

  .btn-cta {
    background: #FBB80C;
    box-shadow: 0.625rem 0.625rem 0.825rem rgba(0, 0, 0, 0.3);
    border-radius: 1.25rem;
    padding: 0.875rem;
    max-width: 39.1875rem;
    display: inline-block;
    font-weight: bold;
    font-size: 1.867375rem;
    line-height: 1.2;
    text-align: center;
    color: #FFFFFF;
    width: 100%;
    transition: all var(--transition);
  }

  .btn-cta b {
    font-weight: bold;
  }

  .btn-cta:hover {
    background-color: #FFFFFF;
    color: #d9a112;
    transition: all var(--transition);
  }

  @media screen and (max-width: 760px) {
    .floating-cta {
      left: 0;
      text-align: center;
    }

    .floating-cta .btn-cta {
      max-width: 90%;
      padding: 2vw;
      font-size: 9vw;
    }
