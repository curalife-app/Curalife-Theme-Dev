  <style>
    @font-face {
      font-family: 'icoFont';
      src: url("{{ 'fonts-icoFont.eot' | asset_url }}");
      src: url("{{ 'fonts-icoFont.eot' | asset_url }}?uayl2p#iefix") format("embedded-opentype"),
      url("{{ 'fonts-icoFont.ttf' | asset_url }}?uayl2p") format("truetype"),
      url("{{ 'fonts-icoFont.woff' | asset_url }}?uayl2p") format("woff"),
      url("{{ 'fonts-icoFont.svg' | asset_url }}?uayl2p#icoFont") format("svg");
      font-weight: normal;
      font-style: normal;
      font-display: block;
    }

    body {
      padding: 0;
    }

    .page-overflow {
      overflow: hidden;
    }

    .container {
      padding: 0 15px;
    }

    section {
      padding: 30px 0;
    }

    h2,
    h3 {
      margin: 0;
      text-align: start;
    }

    h2 {
      font-weight: 400;
      font-size: 40px;
      margin-bottom: 15px;
    }

    h3 {
      font-weight: 300;
      font-size: 22px;
      line-height: 26px;
      margin-bottom: 20px;
    }

    @media screen and (max-width:760px) {
      h2 {
        font-size: 24px;
      }

      h3 {
        font-size: 17px;
      }
    }
  </style>

