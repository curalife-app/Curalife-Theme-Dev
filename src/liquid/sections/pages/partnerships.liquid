<style>
  :root {
    --grey: #333333;
    --green-dark: #404733;
    --green: #9EB069;
    --green-light: #EBF3E3;
    --brown-dark: #A86129;
    --brown: #CB9F6F;
    --brown-light: #F0D4A6;
    --brown-lighter: #FEFBF6;
    --orange: #FFB22D;
  }

  html {
    scroll-behavior: smooth;
  }

	body {
		padding-top: 79px;
	}

  section {
    position: relative;
    padding: 80px 0;
    scroll-margin-top: 80px;
  }

  .container {
    position: relative;
  }

  h1 {
      font-family: DM Sans;
      font-size: 80px;
      margin-top: unset;
      margin-bottom: 0.3em;
  }

  h2 {
    color: var(--grey);
    font-family: DM Sans;
    font-size: 35px;
    font-weight: 700;
    margin-top: 0;
    margin-bottom: 100px;
    max-width: 900px;
  }

  h2.center {
    text-align: center;
    margin: auto;
  }

  h2 span.bigger {
    font-size: 80px;
    font-weight: 700;
  }

  h3 {
    font-family: DM Sans;
    font-size: 24px;
    font-weight: 700;
    margin-top: unset;
    margin-bottom: 10px;
  }

  p {
    font-family: DM Sans;
    font-size: 24px;
    line-height: 1.2em;
    margin: unset;
  }

  p.subtitle {
    font-family: DM Sans;
    font-size: 26px;
    margin-top: -80px;
  }

  button {
    background-color:var(--orange);
    color: white;
    border: 0;
    padding: 20px 40px;
    font-size: 25px;
    font-weight: bold;
  }

  @media screen and (max-width: 768px) {
		body {
			padding-top: 70px;
		}

    section {
      padding: 10vw 0;
    }

    h1 {
        font-size: 13vw;
    }

    h2 {
      text-align: center;
      font-size: 7vw;
      margin-bottom: 10vw;
    }

    h3 {
      font-size: 6vw;
      margin-bottom: 3vw;
    }

    p {
      font-size: 5vw;
    }

    p.subtitle {
      font-size: 5vw;
      margin-top: -8vw;
    }
  }
</style>


<style>
  .shipscout-banner {
    display: none;
  }

  .locale-selectors__container {
    display: none;
  }
</style>

<script>
  AOS.init();
</script>

