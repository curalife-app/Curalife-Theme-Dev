{% comment %}
  
  It is not recommended to make changes to this file as it 
  is overwritten frequently by the app Careers Page Pro

{% endcomment %}

<style>
#smoothie-job-content *{
  box-sizing: border-box;
}
#smoothie-job-content h1{
margin-bottom: 50px;
font-weight:600;
font-size:26px;
text-transform: uppercase;
letter-spacing: 0.1em;
text-align: center;
}
#smoothie-job-content h2,#smoothie-job-content h2 *{
font-size:20px;
font-weight:600;
margin: 26px 0 16px 0;
text-transform: uppercase;
letter-spacing: 0.1em;
}
#smoothie-job-content h3,#smoothie-job-content h3 *{
font-size:18px;
font-weight:600;
text-transform: uppercase;
letter-spacing: 0.1em;
margin: 0 0 18px 0;
}
#smoothie-job-content p{
font-size:16px;
margin-bottom:24px
}
#smoothie-job-content span.detail-text{
float:left;
margin: 4px 0 0 10px;
font-size: 14px;
}
#smoothie-job-content .detail{
margin-right:20px;
margin-bottom:10px;
clear: none;
display:inline-block;
float:left
} 
#smoothie-job-content .details{
display:block;
margin: 5px 0;
}
#smoothie-job-content .details:after{
content:"";
display:block;
clear:both
}
#smoothie-job-content .smoothie-icon{
height:20px;
width:20px
}
#smoothie-job-content .smoothie-icon-wrapper{
display:inline-block
}
#smoothie-job-content span.detail .smoothie-icon-wrapper{
float:left
}
#smoothie-job-content .job-page ul{
padding: 0;
margin: 0;
}
#smoothie-job-content .job-page li{
list-style: none;
padding: 26px 54px;
border-bottom: 1px solid #eee;
cursor: pointer;
}
#smoothie-job-content .job-page li:hover{
background: rgba(0, 0, 0, 0.04);    
}
#smoothie-job-content .job-page li:after{
content: "";
clear: both;
display: block;
}

#smoothie-job-content .job-page{
max-width: 1000px;
padding: 30px;
margin: 0 auto;
width: 90%;
-webkit-box-shadow: 0px 0px 5px 1px rgba(0,0,0,0.5);
box-shadow: 0px 0px 5px 1px rgba(0,0,0,0.5);
margin-bottom: 40px;
}
#smoothie-job-content .job-intro{
  max-width: 1000px;
  padding: 0 30px 30px;
  margin: 0 auto;
}
#smoothie-job-content a > li{
  border-bottom: 1px solid #ddd;
}
#smoothie-job-content a > li:before{
  content: none !important;
}
#smoothie-job-content a:last-child > li{
  border: 0px;
}

p.ql-align-center{
  text-align: center;
}

@media(max-width: 600px){
  #smoothie-job-content .job-page {
    padding: 16px;
  }
  #smoothie-job-content .job-page li {
    padding: 10px 20px;
  }
  
}
#smoothie-job-content p.jobs-empty{
text-align: center;
  margin-top: 20px;
  font-size: 17px;
  color: #888;
}







</style>

{% assign cda = page.content | split: '<span id="smoothie-career-data" style="display: none !important;">' %}
{% assign cdb = cda[1] | split: '</span>' %}

{% assign listings = cdb[0] | split: '<br>' %}

<div id="smoothie-job-content" class="smoothie-search-page" style="">

    

    <h1>Career Opportunities</h1>

    

    
  
  <div class="job-page">
    <ul id="smoothie-search-list">

  {% if listings.size == 0 %}
    <p class="jobs-empty">There are no open jobs currently.</p>
  {% endif %}

  {% for listing in listings %}
      
      {% assign parts = listing  | split: '|||' %}
      {% assign handle = parts[0] %}
      {% assign title = parts[1] %}
      {% assign location = parts[2] %}
      {% assign team = parts[3] %}
      {% assign salary = parts[4] %}
      {% assign deadline = parts[5] %}

      {% assign job_type = "" %}
      {% if parts.size > 5 %}
      {% assign job_type = parts[6] %}
      {% endif %}

      {% assign career_currency = "" %}
      {% assign career_period = "" %}
      {% assign career_amount_type = "" %}
      {% assign career_amount_value = "" %}
      {% assign career_amount_min = "" %}
      {% assign career_amount_max = "" %}
      {% assign career_show_salary = "" %}
      
      {% if parts.size > 12 %}
        {% assign career_currency = parts[7] %}
        {% assign career_period = parts[8] %}
        {% assign career_amount_type = parts[9] %}
        {% assign career_amount_value = parts[10] %}
        {% assign career_amount_min = parts[11] %}
        {% assign career_amount_max = parts[12] %}
        {% assign career_show_salary = parts[13] %}
      {% endif %}

      {% if parts.size > 13 %}
        {% assign external_link = parts[14] %}
      {% endif %}

      {% if parts.size > 14 %}
        {% assign listing_tag = parts[15] %}
      {% endif %}

      {% assign career_period_text = "" %}
      {% if career_period != blank %}
        {% capture career_period_text %}
          {% case career_period %}
          {% when 'HOUR' %}
          hour
          {% when 'DAY' %}
          day
          {% when 'WEEK' %}
          week
          {% when 'MONTH' %}
          month
          {% when 'YEAR' %}
          year
          {% endcase %}
        {% endcapture %}
      {% endif %}
        
      {% if career_show_salary != blank and career_show_salary == 'true' %}

        {% assign size_value = career_amount_value | size %}
        {% assign size_max = career_amount_max | size %}
        {% assign size_min = career_amount_min | size %}
        
        {% if size_value > 3 %}
          {% assign val_slice = career_amount_value | slice: -3, 3 %}
          {% if val_slice == '000' %}
            {% assign size = size_value | minus: 3 %}
            {% assign career_amount_value = career_amount_value | slice: 0, size | append: 'K' %}
          {% endif %}
        {% endif %}
      
        {% if size_max > 3 %}
          {% assign max_slice = career_amount_max | slice: -3, 3 %}
          {% if val_slice == '000' %}
            {% assign size = size_max | minus: 3 %}
            {% assign career_amount_max = career_amount_max | slice: 0, size | append: 'K' %}
          {% endif %}
        {% endif %}
      
        {% if size_min > 3 %}
          {% assign min_slice = career_amount_min | slice: -3, 3 %}
          {% if val_slice == '000' %}
            {% assign size = size_min | minus: 3 %}
            {% assign career_amount_min = career_amount_min | slice: 0, size | append: 'K' %}
          {% endif %}
        {% endif %}
        

        {% assign suffix_string = " " | append: career_currency | downcase %}
        {% assign suffix_string = suffix_string | append: " / " | append: career_period_text | downcase %}

        {% assign salary = "" %}
        {% if career_amount_type == 'single' %}
          {% assign salary = salary | append: career_amount_value %}
        {% elsif career_amount_type == 'range' %}
          {% assign salary = salary | append: career_amount_min %}
          {% assign salary = salary | append: " - " %}
          {% assign salary = salary | append: career_amount_max %}
        {% endif %}

        {% assign salary = salary | append: suffix_string %}
    
      {% endif %}

    {% if external_link == blank or external_link == 'null' %}
    <a href="/pages/{{ handle }}" >
    {% else %}
    <a href="{{ external_link }}" target="_blank">
    {% endif %}
    
        <li class="smoothie-job-listing-card" location="{{ location }}" job-type="{{ job_type }}" team="{{ team }}" {% if listing_tag != blank %} tag="{{ listing_tag }}"{% endif %}>
          <h3>{{ title }}</h3>

          {% if location != "" and location != "null" %}
          <span class="detail" title="Career location">
            <div class="smoothie-icon-wrapper">
              <svg class="smoothie-icon" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg"><title></title><g data-name="53-Location" id="_53-Location"><path class="cls-1" style="fill:none;stroke:#000;stroke-linejoin:round;stroke-width:1.3px;" d="M27,12A11,11,0,0,0,5,12c0,9,10,19,10,19S27,22,27,12Z"></path><circle style="fill:none;stroke:#000;stroke-linejoin:round;stroke-width:1.3px;" cx="16" cy="11" r="4"></circle></g></svg>
            </div>
            <span class="detail-text" style="margin-left: 7px;">{{ location }}</span>
          </span>
          {% endif %}

