<div class="health-page">
    <div class="container">
        <div class="egg-wrapper">
           
           <div class="egg-titles">
            <div class="egg-subtitle">{{ section.settings.subtitle }}</div>
            <div class="egg-title"> <h1>{{ section.settings.title }}</h1></div>
             </div>
            <div class="img-egg">
                <img src="{{ section.settings.image | image_url }}" width="1178" height="722" alt="">
            </div>
        </div>
       
    </div>
   
</div>


<style>
   .container {
      position: relative;
      width: 100%;
      height: 100%;
      max-width: 1200px;
      margin: 0 auto;
      padding: 0 15px;
  }
</style>


<style>
    .egg-wrapper {
        display: flex;
        align-items: center;
        justify-content: center;
    }

    
</style>



{% schema %}
{
    "name": "Health",
    "tag": "section",
    "class": "health",
    "disabled_on": {
        "groups": [
            "header",
            "footer"
        ]
    },
    "settings": [{
        "type": "text",
        "id": "title",
        "label": "Heading"
    },
    {
        "type": "image_picker",
        "id": "image",
        "label": "Image"
    },
    {
      "type": "text",
      "id": "subtitle",
      "label": "Subtitle"
    },
    {
        "type": "richtext",
        "id": "main_content",
        "label": "Content"
    }
],
    "presets": [{
        "name": "Health"
    }]
}
{% endschema %}