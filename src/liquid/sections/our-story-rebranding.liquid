{% render 'our-story-curalife' %} 
{% render 'our-story-son-father', main_image: section.settings.image, main_title: section.settings.title, subtitle: section.settings.subtitle, main_content: section.settings.main_content %}
{% render 'our-story-dna', main_image: section.settings.image, main_title: section.settings.title, subtitle: section.settings.subtitle, main_content: section.settings.main_content %}
<style>
  .container {
    position: relative;
    width: 100%;
    height: 100%;
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 15px;
  }
</style>

{% schema %}
{
	"name": "Rebranding T&I",
	"tag": "section",
	"class": "rebranding-t-i",
	"disabled_on": {
		"groups": [
			"header",
			"footer"
		]
	},
	"settings": [{
			"type": "color",
			"id": "bg_color",
			"label": "Background Color"
		},
		{
			"type": "image_picker",
			"id": "image",
			"label": "Image"
		},
		{
			"type": "text",
			"id": "title",
			"label": "Title"
		},
    {
      "type": "text",
      "id": "subtitle",
      "label": "Subtitle"
    },
		{
			"type": "richtext",
			"id": "main_content",
			"label": "Content"
		}
	],
	"presets": [{
		"name": "Rebranding T&I"
	}]
}
{% endschema %}