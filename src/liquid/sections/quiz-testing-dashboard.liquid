{% comment %}
	Quiz Testing Dashboard Section
	For Curalife team to easily test all quiz scenarios
{% endcomment %}

<style>
	.quiz-dashboard {
		max-width: 1200px;
		margin: 0 auto;
		padding: 40px 20px;
		font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
	}

	.quiz-dashboard-header {
		text-align: center;
		margin-bottom: 48px;
	}

	.quiz-dashboard-title {
		font-size: 2.5rem;
		font-weight: 700;
		color: #306e51;
		margin-bottom: 12px;
	}

	.quiz-dashboard-subtitle {
		font-size: 1.2rem;
		color: #6b7280;
		margin-bottom: 24px;
	}

	.quiz-dashboard-status {
		display: inline-flex;
		align-items: center;
		gap: 8px;
		background: #f0fdf4;
		border: 1px solid #bbf7d0;
		color: #166534;
		padding: 8px 16px;
		border-radius: 24px;
		font-size: 0.875rem;
		font-weight: 500;
	}

	.quiz-dashboard-grid {
		display: grid;
		grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
		gap: 32px;
		margin-bottom: 48px;
	}

	.quiz-test-category {
		background: white;
		border-radius: 16px;
		padding: 32px;
		box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
		border: 1px solid #e5e7eb;
	}

	.quiz-category-header {
		display: flex;
		align-items: center;
		gap: 12px;
		margin-bottom: 24px;
	}

	.quiz-category-icon {
		width: 48px;
		height: 48px;
		border-radius: 12px;
		display: flex;
		align-items: center;
		justify-content: center;
		font-size: 24px;
	}

	.quiz-category-icon.success {
		background: #dcfce7;
		color: #166534;
	}

	.quiz-category-icon.error {
		background: #fef2f2;
		color: #dc2626;
	}

	.quiz-category-icon.warning {
		background: #fffbeb;
		color: #d97706;
	}

	.quiz-category-title {
		font-size: 1.5rem;
		font-weight: 600;
		color: #111827;
		margin: 0;
	}

	.quiz-category-description {
		color: #6b7280;
		font-size: 0.95rem;
		margin-bottom: 24px;
		line-height: 1.5;
	}

	.quiz-test-buttons {
		display: flex;
		flex-direction: column;
		gap: 12px;
	}

	.quiz-test-button {
		display: flex;
		align-items: center;
		justify-content: space-between;
		padding: 16px 20px;
		background: #f9fafb;
		border: 1px solid #e5e7eb;
		border-radius: 12px;
		text-decoration: none;
		transition: all 0.2s ease;
		position: relative;
		overflow: hidden;
	}

	.quiz-test-button:hover {
		background: #f3f4f6;
		border-color: #d1d5db;
		transform: translateY(-1px);
		box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.1);
	}

	.quiz-test-button.success:hover {
		background: #f0fdf4;
		border-color: #bbf7d0;
	}

	.quiz-test-button.error:hover {
		background: #fef2f2;
		border-color: #fecaca;
	}

	.quiz-test-button.warning:hover {
		background: #fffbeb;
		border-color: #fed7aa;
	}

	.quiz-test-info {
		flex: 1;
	}

	.quiz-test-name {
		font-weight: 600;
		color: #111827;
		font-size: 1rem;
		margin-bottom: 4px;
	}

	.quiz-test-details {
		font-size: 0.875rem;
		color: #6b7280;
		margin-bottom: 6px;
	}

	.quiz-test-member-id {
		font-family: 'Monaco', 'Consolas', monospace;
		font-size: 0.8rem;
		color: #4b5563;
		background: #f3f4f6;
		padding: 2px 6px;
		border-radius: 4px;
		display: inline-block;
	}

	.quiz-test-action {
		display: flex;
		align-items: center;
		gap: 8px;
		color: #6b7280;
		font-size: 0.875rem;
		font-weight: 500;
	}

	.quiz-test-action svg {
		width: 18px;
		height: 18px;
	}

	.quiz-footer {
		text-align: center;
		padding: 32px 20px;
		background: #f9fafb;
		border-radius: 12px;
		border: 1px solid #e5e7eb;
	}

	.quiz-footer-title {
		font-size: 1.25rem;
		font-weight: 600;
		color: #111827;
		margin-bottom: 12px;
	}

	.quiz-footer-text {
		color: #6b7280;
		margin-bottom: 20px;
		line-height: 1.6;
	}

	.quiz-docs-link {
		display: inline-flex;
		align-items: center;
		gap: 8px;
		background: #306e51;
		color: white;
		padding: 12px 24px;
		border-radius: 8px;
		text-decoration: none;
		font-weight: 500;
		transition: background-color 0.2s ease;
	}

	.quiz-docs-link:hover {
		background: #275a42;
	}

	@media (max-width: 768px) {
		.quiz-dashboard {
			padding: 24px 16px;
		}

		.quiz-dashboard-grid {
			grid-template-columns: 1fr;
			gap: 24px;
		}

		.quiz-test-category {
			padding: 24px;
		}

		.quiz-dashboard-title {
			font-size: 2rem;
		}
	}
</style>

